<p id="notice"><%= notice %></p>

<h1>State Provinces</h1>

<table>
  <thead>
    <tr>
      <th>State province name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @state_provinces.each do |state_province| %>
      <tr>
        <td><%= state_province.state_province_name %></td>
        <td><%= link_to 'Show', state_province %></td>
        <td><%= link_to 'Edit', edit_state_province_path(state_province) %></td>
        <td><%= link_to 'Destroy', state_province, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New State Province', new_state_province_path %>
